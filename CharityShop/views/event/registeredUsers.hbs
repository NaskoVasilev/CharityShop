<section id="all-categories">
    <table>
        <tr>
            <th>Username</th>
            <th>Name</th>
            <th>Surname</th>
            <th>Email</th>
        </tr>
        {{#each users}}
            <tr>
                <td>{{username}}</td>
                <td>{{firstName}}</td>
                <td>{{lastName}}</td>
                <td>{{email}}</td>
            </tr>
        {{/each}}
    </table>
</section>